/* 触碰变红 */
.red:hover{
	color: #d7063b;
}

.point:hover{
	color: #d7063b;
	text-decoration: underline;
}

/* 头部 */
.header{
	width: 100%;
}
	/* 头部广告 */
	.header .ad{
		background:rgb(255, 156, 0);
		height: 60px;
		
	}

	.header .ad .outer{
		display: block;
		width: 1190px;
		margin: 0 auto;
		position: relative;
		z-index: 0;
	}

	.header .ad #spread{
		top: 0;
		width: 20px;
	    height: 42px;
	    position: absolute;
	    right: 20px;
	    background:url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) no-repeat;
	    background-position: -52px 0;
	    z-index: 1;
	}

	.header .ad #close{
		top: 0;
	    right: 0;
		width: 16px;
	    height: 16px;
	    position: absolute;
	    z-index: 1;
	    background:url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) 0 -96px no-repeat;
	}
	

	/* 头部 导航栏 */
	.header .top_bar{
		width: 100%;
	    height: 35px;
	    line-height: 35px;
	    border-bottom: 1px solid #dedede;
	    background-color: #f7f7f7;
	}

	.header .top_bar .bar_center{
		width: 1190px;
		height: 100%;
		margin: 0 auto;
	}

	/* 导航栏 左边 */

	.header .top_bar .bar_center .lf{
		float: left;
		height: 100%;
	}

	.header .top_bar .bar_center li{
		float: left;
		position: relative;
	}

	.header .top_bar .bar_center .lf li .regi{
		margin-left: 5px;
	}

	.header .top_bar .bar_center .line{
		width: 1px;
	    height: 10px;
	    padding: 0;
	    background: #dedede;
	    margin: 0 11px;
	    margin-top: 13px;

	}

	.header .top_bar .bar_center .lf li .arrow{
		width: 7px;
	    height: 4px;
	    top: 16px;
	    right: -10px;
	    position: absolute;

	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) -140px -28px no-repeat;
	}

	/* 导航栏 右边 */

	.header .top_bar .bar_center .rt{
		float: right;
		height: 100%;
	}

	.header .top_bar .bar_center .rt li .arrow{
		width: 7px;
	    height: 4px;
	    display: inline-block;
	    position: relative;
	    top: -3px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) -140px -28px no-repeat;
	}

	.header .top_bar .bar_center .rt .charging{
		width: 11px;
	    height: 16px;
	    display: inline-block;
	    margin-bottom: -3px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) -36px -96px no-repeat;
	}

	.header .top_bar .bar_center .rt .mob{
		width: 10px;
	    height: 16px;
	    margin-bottom: -3px;
	    display: inline-block;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) -136px -44px no-repeat;
	}

	.header .top_bar .bar_center .rt .sina{
		margin-right: 10px;
		width: 18px;
	    height: 18px;
	    float: right;
	    margin-top: 8px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) no-repeat;
    	background-position: -109px -75px;
	}

	.header .top_bar .bar_center .rt .wechat{
		margin-right: 10px;
		width: 18px;
	    height: 18px;
	    float: right;
	    margin-top: 8px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) no-repeat;
    	background-position: -89px -75px;
	}


	/* 头部 中间搜索部分 */

	.header .center_main{
		width: 100%;
		height: auto;
	}

	.header .center_main .center_wrap{
		width: 1190px;
		overflow: hidden;
		margin: 0 auto;
	}

	.header .center_main .center_wrap{
		width: 1190px;
		height: auto;
		padding-top: 20px;
		margin: 0 auto;
	}
	
	/* LOGO部分 */

	.header .center_main .center_wrap .logo{
		width: 360px;
		float: left;
	}

	.header .center_main .center_wrap .logo a{
		display: inline-block;
	}

	.header .center_main .center_wrap .logo a img{
		display: block;
	}

	/* 搜索框部分 */

	.header .center_main .center_wrap .search_wrap{
		float: left;
		width: 540px;
	}

	.header .center_main .center_wrap .search_wrap .input_wrap{
		width: 503px;
	    height: 34px;
	    border: 2px solid #d7063b;
	    padding-left: 33px;
	    position: relative;
	}

	.header .center_main .center_wrap .search_wrap .search_icon{
		width: 21px;
	    height: 21px;
	    position: absolute;
	    top: 7px;
	    left: 8px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) 0 -52px no-repeat;
	}

	.header .center_main .center_wrap .search_wrap #search_txt{
		border: 0;
		width: 395px;
		padding: 5px;
		height: 24px;
		outline: 0;
		font-size: 14px;
	}

	.header .center_main .center_wrap .search_wrap #search_btn{
		border: 0;
		outline: 0;
		float: right;
		width: 98px;
		height: 34px;
		background-color: #d7063b;
		font-size: 18px;
		color: #fff;
		letter-spacing: 7px;
		padding-left: 13px;
		cursor: pointer;
	}

	/* 关键词 */

	.header .center_main .center_wrap .search_wrap .point_words{
		padding-top: 5px;
	}

	.header .center_main .center_wrap .search_wrap .point_words a{
		margin-right: 5px;
		color: #999;
	}

	.header .center_main .center_wrap .search_wrap .point_words a:hover{
		color: #d7063b;
		text-decoration: underline;
	}

	/* 右边 购物车 */

	.header .center_main .center_wrap .rt_cart{
		float: right;
		margin-top: -3px;
		margin-right: 30px;
	}

	.header .center_main .center_wrap .rt_cart #cart{
		float: left;
		width: 78px;
		height: 43px;
		border: 1px solid #ddd;
		background-color: #e9e9e9;
		text-align: center;
		line-height: 43px;
	}

	.header .center_main .center_wrap .rt_cart .cart_wrap{
		float: left;
		width: 58px;
		height: 45px;
		background: #d7063b;
		position: relative;
		line-height: 45px;
		text-align: center;
		color: #fff;
		padding-left: 6px;
	}

	.header .center_main .center_wrap .rt_cart .cart_wrap i{
		width: 18px;
	    height: 15px;
	    position: absolute;
	    top: 15px;
	    left: 8px;
	    margin-right: 3px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) -72px -96px no-repeat;
	}

	.header .center_main .center_wrap .rt_cart .cart_wrap span{
		margin: 0 -2px
	}

	.header .center_main .center_wrap .rt_cart .cart_wrap #cart_num{
		font-weight: bold;
	}


	/* 头部 底层商品分类 */

	.header .center_main .bottom_wrap{
		width: 1190px;
		/* overflow: hidden; */
		height: 40px;
		margin: 0 auto;
		margin-top: 12px;
	}
	
	/* 商品分类 */

	.header .center_main .bottom_wrap .goods_sort{
		width: 190px;
		height: 40px;
		background: #d7063b;
		float: left;
		position: relative;
	}

	.header .center_main .bottom_wrap .goods_sort .sort_icon{
		width: 16px;
	    height: 10px;
	    display: inline-block;
	    overflow: hidden;
	    position: absolute;
	    top: 15px;
	    left: 10px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) 0 -114px no-repeat;
	}

	.header .center_main .bottom_wrap .goods_sort .title{
		height: 100%;
		display: block;
		color: #fff;
		font-size: 14px;
		line-height: 40px;
		padding-left: 38px;
		font-weight: bold;

	}
	
	/* 商品列表 */

	.header .center_main .bottom_wrap .good_list {
		width: 100%;
		background: rgba(248, 248, 248, 0.95);
		padding: 2px 0;
		position: absolute;
		top: 40px;
		height: 450px;
		z-index: 9999;
	}

	.header .center_main .bottom_wrap .good_list .item{
		height: 30px;
		width: 182px;
		line-height: 30px;
		font-size: 13px;
		padding-left: 8px;
		transition: all .3s;
	}

	.header .center_main .bottom_wrap .good_list .item:hover{
		background: #d7063b;
		padding-left: 12px;
		width: 178px;
		
	}

	.header .center_main .bottom_wrap .good_list .item:hover a{
		color: #fff;
	}

	.header .center_main .bottom_wrap .good_list .item i{
		height: 30px;
		width: 182px;
		line-height: 30px;
		font-size: 13px;
		padding-left: 8px;
	}

	/* 主导航 */

	.header .center_main .bottom_wrap .main_nav{
		height: 100%;
		float: left;
	}

	.header .center_main .bottom_wrap .main_nav li{
		float: left;
		position: relative;
		height: 100%;
		line-height: 40px;
	}

	.header .center_main .bottom_wrap .main_nav li a{
		display: block;
		padding: 0 15px;
		height: 100%;
		font-size: 15px;
		font-weight: 700;

	}

	.header .center_main .bottom_wrap .main_nav li .hot{
		display: block;
	    width: 27px;
	    height: 18px;
	    position: absolute;
	    top: -7px;
	    right: -2px;
	    background: url(http://static02.fn-mart.com/product/images/index/fn_g_header.png?170725) no-repeat;
        background-position: 0 -140px;
	}


	/* 右侧广告 */

	.header .center_main .bottom_wrap .goods_ad{
		float: right;
		position: relative;
		top: -25px;
		right: 28px;
	}


/* banner */

.banner{
	width: 100%;
	height: auto;
	position: relative;
}


		/* 轮播图 */

		.banner #scroll_wrap{
			width: 100%;
			height: auto;
			position: relative;
			height: 454px;
		}
 
		.banner #scroll_wrap li{
			width: 100%;
			height: auto;
			position: absolute;
			z-index: 0;
		}

		.banner #scroll_wrap li a{
			width: 1190px;
			height: 454px;
			display: block;
			margin: 0 auto;
		}

		.banner #scroll_wrap li a img{
			margin-left: 190px;
			float: left;
		}

		/* 轮播图小按钮 */

		.banner #scroll_wrap #points_toggle{
			position: absolute;
			bottom: 0;
			left: 50%;
			transform: translate(-50%);
			z-index: 9999;
		}

		.banner #scroll_wrap #points_toggle a{
			width: 19px;
		    height: 19px;
		    line-height: 19px;
		    text-align: center;
		    display: inline-block;
		    color: #fff;
		    font-size: 14px;
		    margin-right: 10px;
		    border-radius: 50%;
		    overflow: hidden;
		    background: url(http://static02.fn-mart.com/product/images/index/fn_index.png?170725) 0 -141px no-repeat;
		}

		.banner #scroll_wrap .rt_click,.banner #scroll_wrap .lf_click{
			width: 28px;
			height: 62px;
			position: absolute;
			z-index: 9999;
			color: #fff;
			background: #000;
			font-size: 22px;
			line-height: 62px;
			text-align: center;
			top: 50%;
			transform: translate(0,-50%);
			opacity: 0.3;
			display: none;
			cursor: pointer;
		}

		.banner #scroll_wrap .lf_click{
			left: 318px;
		}

		.banner #scroll_wrap .rt_click{
			right: 330px;
		}

		/* 右侧功能区域 */
		.banner .rt_bar{
			width: 200px;
			height: 434px;
			background: #f9f9f9;
			position: absolute;
			z-index: 9999;
			left:50%;
			top: 10px;
			margin-left: 395px;
			opacity: .95;
		}

		.banner .rt_bar .bar_sort{
			width: 100%;
			float: left;
			height: 87px;
		}

		.banner .rt_bar .bar_sort li{
			float: left;
			padding-left: 5px;
			padding-top: 10px;
		}

		.banner .rt_bar .bar_sort a{
			display: inline-block;
			width: 60px;
			height: 73px;
			text-align: center;
		}

		.banner .rt_bar .bar_sort a:hover p{
			color: #d7063b;
		}

		.banner .rt_bar .bar_sort li img{
			display: inline-block;
		}

		.banner .rt_bar .events{
			border-top: 1px solid #ddd;
			width: 190px;
		    height: 44px;
		    padding: 7px 0 7px 10px;
		    overflow: hidden;
		}
		
		.banner .rt_bar .events p{
			width: 190px;
		    overflow: hidden;
		    height: 22px;
		    line-height: 22px;
		}


		/* 充值区域 */

		.banner .rt_bar .mid_slide {
			border-top: 1px solid #ddd;
			width: 100%;
		    position: relative;
		    height: 174px;
		}

		.banner .rt_bar .mid_slide li{
			float: left;
			display: block;
		    height: 27px;
		    line-height: 28px;
		    text-align: center;
		    font-size: 12px;
		    color: #333;
		    font-family: "Microsoft YaHei";
		    width: 99px;
		    border-left: 1px solid #ddd;
		    border-bottom: 1px solid #ddd;
		    cursor: pointer;
		}

		.banner .rt_bar .mid_slide .charging_active{
			color: #d7063b;
		    line-height: 24px;
		    height: 26px;
		    border-top: 2px solid #d7063b;
		    border-bottom: 0 none;
		}

		.banner .rt_bar .mid_slide .ph_money,.banner .rt_bar .mid_slide .liu_money{
			position: absolute;
			top: 28px;
			width: 180px;
			padding: 10px 10px 0 10px;
			height: 135px;
		}

		.banner .rt_bar .mid_slide input{
			width: 173px;
			height: 14px;
			border: 1px solid #dedede;
		    background: #fff;
		    position: relative;
		    font-size: 12px;
		    font-family: "Microsoft YaHei";
		    color: #3c3c3c;
		    padding: 5px 0 5px 5px;
		    outline: 0;
		    border-bottom: 0;
		}

		.banner .rt_bar .mid_slide p{
			width: 173px;
		    height: 14px;
		    border: 1px solid #dedede;
		    background: #fff;
		    position: relative;
		    font-size: 12px;
		    padding: 5px 0 5px 5px;
		    font-family: "Microsoft YaHei";
		    
		}

		.banner .rt_bar .mid_slide .num_type{
			display: block;
			cursor: pointer;
		}

		.banner .rt_bar .mid_slide i{
			display: block;
		    width: 7px;
		    height: 7px;
		    position: absolute;
		    top: 8px;
		    right: 7px;
		    background: url(http://static02.fn-mart.com/product/images/index/fn_index.png?170725) -29px -162px no-repeat;
		    cursor: pointer;
		}

		.banner .rt_bar .mid_slide .selector{
			width: 180px;
			height: 125px;
			max-height: 130px;
			overflow-y: scroll;
			overflow-x: hidden;
			position: absolute;
			z-index: 9999;
			background: #fff;
			border: 1px solid #aaa;
			display: none;
		}

		.banner .rt_bar .mid_slide .selector li{
			width: 180px;
			height: 20px;
			text-align: left;
			padding: 5px 0 5px 10px;
			line-height: 20px;
			border: 0;
			color: #000;
		}

		.banner .rt_bar .mid_slide .selector li a{
			color: #000;
		}

		.banner .rt_bar .mid_slide .selector .li_active{
			background: #ffece5;
		}

		.banner .rt_bar .mid_slide .price_wrap{
			margin-top: 6px;
			display: block;
			color: #999;
		}
		
		.banner .rt_bar .mid_slide .price_wrap span{
			color: #d7063b;
		}

		.banner .rt_bar .mid_slide .tip{
			width: 180px;
		    height: 18px;
		    font-size: 12px;
		    font-family: "Microsoft YaHei";
		}

		.banner .rt_bar .mid_slide .charging_btn{
			border: 0;
			display: block;
		    width: 80px;
		    height: 28px;
		    line-height: 28px;
		    text-align: center;
		    font-size: 12px;
		    color: #fff;
		    font-family: "Microsoft YaHei";
		    background: #d7063b;
		    margin-top: 2px;
		}

		.banner .rt_bar .hotsale{
			opacity: 0.8
		}
		

		

		


		

	

